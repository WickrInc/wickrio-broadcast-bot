{
  "apps": [
    {
      "name": "WickrIO-Broadcast-Bot",
      "args": [],
      "script": "./build/index.js",
      "exec_interpreter": "node",
      "autorestart": false,
      "watch": ["package.json"],
      "ignore_watch": [".git"],
      "env": {
        "log_tokens": {
          "LOG_LEVEL": {
            "value": "info"
          },
          "LOG_FILE_SIZE": {
            "value": "10m"
          },
          "LOG_MAX_FILES": {
            "value": "5"
          }
        },
        "tokens": {}
      },
      "out_file": "log.output",
      "error_file": "err.output"
    }
  ]
}
